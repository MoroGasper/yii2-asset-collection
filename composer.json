{
    "name": "p2made/yii2-asset-collection",
    "description": "A bunch of CSS & JS resources wrapped as Yii 2 AssetBundles.",
    "keywords": [
        "yii", "yii2", "PHP",
        "asset", "bundle",
        "menu", "metisMenu",
        "Bootstrap Social", "social",
        "dataTables", "data", "tables",
        "Flot", "Morris", "Morris.JS", "charts", "excanvas",
        "raphael", "graphics", "vector", "vector graphics",
        "SweetAlert", "alerts",
        "moment.js", "time", "moment", "timezone", "moment-timezone"
        "html5shiv", "Respond.JS"
    ],
    "authors": [
        {
            "name": "Pedro Plowman",
            "web": "https://github.com/p2made"
        }
    ],
    "type": "yii2-extension",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Pedro Plwoman",
            "email": "pedro@p2made.net.au",
            "homepage": "http://p2made.net.au/"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.*",
        "bower-asset/moment" : "*",
        "bower-asset/moment-timezone" : "*",
        "datatables/datatables": "1.*",
        "kartik-v/yii2-datecontrol": "*",
        "kartik-v/yii2-field-range": "*",
        "kartik-v/yii2-social": "*",
        "kartik-v/yii2-widgets": "*",
        "onokumus/metismenu": "2.*",
        "philippfrenzel/yii2fullcalendar": "*",
        "rmrevin/yii2-fontawesome": "2.9.*"
    },
    "autoload": {
        "psr-4": {
            "p2made\\assets\\": ""
        }
    }
}
