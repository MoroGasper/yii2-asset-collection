{
    "name": "p2made/yii2-asset-collection",
    "description": "A bunch of CSS & JS resources wrapped as Yii 2 AssetBundles.",
    "type": "yii2-extension",
    "keywords": [
        "yii2", "yii", "extension", "PHP",
        "asset", "bundle",
        "menu", "metisMenu",
        "Bootstrap Social", "social",
        "dataTables", "data", "tables",
        "Flot", "Morris", "Morris.JS", "charts", "excanvas",
        "raphael", "graphics", "vector", "vector graphics",
        "SweetAlert", "alerts", "Holder.js", "placeholder", "placeholder-image",
        "moment.js", "time", "moment", "timezone", "moment-timezone",
        "FullCalendar", "calendar",
        "html5shiv", "Respond.JS"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pedro Plowman",
            "email": "p2opensource@gmail.com",
            "homepage": "https://github.com/p2made"
        }
    ],

    "version": "2.6.0",
    "minimum-stability": "dev",

    "require": {
        "yiisoft/yii2": "2.0.*",
        "yiisoft/yii2-jui": "*",
        "bower-asset/fullcalendar" : "2.3.*",
        "bower-asset/isotope" : "*",
        "bower-asset/moment" : "*",
        "bower-asset/moment-timezone" : "*",
        "bower-asset/sweetalert" : "*",
        "datatables/datatables": "1.*",
        "fortawesome/font-awesome": "4.3.*",
        "imsky/holder" : "*",
        "onokumus/metismenu": "2.*",
        "philippfrenzel/yii2fullcalendar": "*"
    },
    "autoload": {
        "psr-4": {
            "p2made\\assets\\": ""
        }
    }
}
